{
  "name": "throw-db",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "scrape": "node ./scrape/steps/all.js",
    "scrape:profiles": "node ./scrape/steps/profiles.js",
    "scrape:throws": "node ./scrape/steps/throws.js",
    "scrape:opponents": "node ./scrape/steps/opponents.js",
    "scrape:data": "node ./scrape/steps/data.js",
    "build": "node ./client/index.js",
    "serve": "npm run build && http-server dist -p 8080 -c-1 -e html",
    "watch": "nodemon --watch client --watch data --ext html,css,js,md,json,db --exec \"npm run build\"",
    "local": "npm run serve & npm run watch && kill $(jobs -p)"
  },
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "cssnano": "^7.0.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.0.1",
    "glob": "^10.3.10",
    "marked": "^12.0.2",
    "marked-gfm-heading-id": "^3.1.3",
    "mustache": "^4.2.0",
    "postcss": "^8.4.38",
    "puppeteer": "^23.3.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "nodemon": "^3.1.4"
  }
}
